{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "redirects": [
      { "source": "/old-path", "destination": "/new-path", "permanent": true }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
}
  